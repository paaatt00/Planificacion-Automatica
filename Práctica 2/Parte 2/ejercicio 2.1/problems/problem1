( defproblem problem dronedom

    ( ; INITIAL STATE
        ; drone1 - drone
        (DRONE drone1)
        ; warehouse loc1 - location
        (LOCATION warehouse) (LOCATION loc1)
        ; carrier1 - carrier
        (CARRIER carrier1)
        ; capacity
        (CAPACITY carrier1 10)
        ; coste inicial
        ;(COSTE cost)

        ;(travel-cost cost 0)
        (drone-at drone1 warehouse)
        (drone-free drone1)
        (loc-needs loc1 50)
        (carrier-at carrier1 warehouse)
    )

    ( ; TASKS
        (send-all-boxes) ; enviar-todo
    )
    
)
